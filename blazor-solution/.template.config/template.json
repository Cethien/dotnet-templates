{
	"author": "Cethien",
	"classifications": ["Web", "Blazor"],
	"identity": "BlazorSolution",
	"name": "Blazor Solution",
	"shortName": "blazorsln",
	"tags": {
		"type": "solution",
		"language": "C#"
	},
	"sourceName": "BlazorSolution",
	"sources": [
		{
			"modifiers": [
				{
					"exclude": [
						"**/.vs/**/*",
						"**/.vscode/**/*",
						"**/.idea/**/*",
						"**/.github/**/*",
						"**/.devcontainer/**/*",
						"**/[Bb]in/**",
						"**/[Oo]bj/**",
						".template.config/**/*",
						"**/*.filelist",
						"**/*.user",
						"**/*.lock.json",
						"**/*-lock.json",
						"**/node_modules/**"
					]
				}
			]
		}
	],
	"preferNameDirectory": true,
	"primaryOutputs": [
		{
			"path": "src/BlazorSolution/BlazorSolution.csproj"
		},
		{
			"path": "src/BlazorSolution.Core/BlazorSolution.Core.csproj"
		}
	],
	"postActions": [
		{
			"actionId": "210D431B-A78B-4D2F-B762-4ED3E3EA9025",
			"description": "Restore NuGet packages required by this project.",
			"manualInstructions": [
				{
					"text": "Run 'dotnet restore'"
				}
			],
			"continueOnError": true
		}
	]
}
